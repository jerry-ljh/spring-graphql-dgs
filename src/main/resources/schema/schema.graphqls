type Query {
    shows: [Show!]!
    movies: [Movie!]!
}

type Show {
    title: String!
    actors: [Actor!]!
    music: Music!
    location: Location
}

type Actor {
    name: String!
    age: Int!
}

type Music {
    name: String!
}

type Location {
    name: String!
}

type Movie {
    title: String!
    actors: [Actor!]!
    music: Music!
}